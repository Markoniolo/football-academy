-
  const menu = [
    { text: 'programs', link: 'index.html#programs', type: 'anchor'},
    { text: 'about', link: 'index.html#about', type: 'anchor'},
    { text: 'coaching', link: 'index.html#coaching', type: 'anchor'},
    { text: 'Live', link: '/news.html', type: ''},
    { text: 'approach', link: '/approach.html', type: ''},
    { text: 'contacts', link: '/contact.html', type: ''},
  ]

.menu.js-menu
  .menu__inner
    .menu__list.js-menu-list
      each item in menu
        a.menu__item.btn-black(href=item.link class = item.type ? 'js-menu__item_anchor' : false)= item.text
    .menu__button.js-menu-button
      svg.menu__button-close(viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg')
        path(d='M19 6.41L17.59 5L12 10.59L6.41 5L5 6.41L10.59 12L5 17.59L6.41 19L12 13.41L17.59 19L19 17.59L13.41 12L19 6.41Z' fill='#00000E')
      svg.menu__button-burger(width='24' height='24' viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg')
        path(d='M3 18H21V16H3V18ZM3 13H21V11H3V13ZM3 6V8H21V6H3Z' fill='#00000E')
      | menu
